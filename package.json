{
  "name": "mochi",
  "version": "1.0.0",
  "description": "",
  "main": "js/scripts.js",
  "scripts": {
    "start": "http-server",
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --watch --progress --hot",
    "dev-server": "WEBPACK_ENV=dev webpack-dev-server --watch --progress --hot --inline",
    "deploy": "aws s3 sync . s3://mochibox.io/ --delete --exclude 'js/*' --exclude 'aws/*' --exclude 'old_img/*' --exclude '.git/*' --exclude 'node_modules/*' --exclude 'build/*.json' --exclude 'package.json' --exclude 'webpack.config.js' --exclude '.DS_Store' --exclude '.babelrc' --exclude '.eslintrc' --exclude '.gitignore' --exclude 'npm-debug.log'",
    "invalidate": "aws cloudfront create-invalidation --distribution-id EEIQEWVD8ACU4 --paths /"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewheekin/mochi.git"
  },
  "author": "andrew heekin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewheekin/mochi/issues"
  },
  "homepage": "https://github.com/andrewheekin/mochi#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "page": "^1.7.1"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "4.1.3",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "babili-webpack-plugin": "0.0.11",
    "css-loader": "^0.27.3",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
