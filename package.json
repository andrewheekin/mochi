{
  "name": "mochi",
  "version": "1.0.0",
  "description": "",
  "main": "js/scripts.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --watch --progress --hot",
    "deploy": "aws s3 sync . s3://mochibox.io/ --delete --exclude 'js/*' --exclude 'aws/*' --exclude 'old_img/*' --exclude '.git/*' --exclude 'node_modules/*' --exclude 'build/*.json' --exclude 'package.json' --exclude 'webpack.config.js' --exclude '.DS_Store' --exclude '.babelrc' --exclude '.eslintrc' --exclude '.gitignore' --exclude 'npm-debug.log'",
    "invalidate": "aws cloudfront create-invalidation --distribution-id EEIQEWVD8ACU4 --paths /"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewheekin/mochi.git"
  },
  "author": "andrew heekin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewheekin/mochi/issues"
  },
  "homepage": "https://github.com/andrewheekin/mochi#readme",
  "dependencies": {
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "amazon-cognito-identity-js": "^1.19.0",
    "aws-sdk": "^2.67.0",
    "navigo": "^5.1.0",
    "prismjs": "^1.6.0"    
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
